<script>

   
	fillCity();
	if(undefined!=$("#province").attr("id")){
		var a=$("#myprovince").val();
		var b=$("#mycity").val();
		if(a!=""){
			$("#province")[0].value = a;
			setCity(a);
			if(b!=""){
				$("#city")[0].value = b;
			}

		}
	}
	
	
	function sms_submit(){
		var msg='';
		if(jQuery("#province").val()==''){
			msg='请选择省份';
		}
		if(jQuery("#mobile").val()==''){
			msg='请填写联系方式';
		}else{
			if(!check_mobile(jQuery("#mobile").val())){
			msg='请输入正确的手机号码';
			}	
		}
		if(jQuery("#username").val()==''){
			msg='请填写姓名';
		}
		if(msg!=''){
			alert(msg);
		}else{
		
		jQuery.ajax({
         type : "post",
         url : "../../dealer.auto.ifeng.com/sms/messageHander",
		 dataType:"jsonp",
		 jsonp:"callbackparam",
		 
         data : {"messageInfo.userName":jQuery("#username").val(),"messageInfo.mobile":jQuery("#mobile").val(),"messageInfo.province":jQuery("#province").val(),"messageInfo.city":jQuery("#city").val(),"messageInfo.brandId":"","messageInfo.serialId":"","messageInfo.carId":"","messageInfo.carName":""+""+' '+"","messageInfo.brandName":"","messageInfo.serialName":""+"","messageInfo.cityName":jQuery("#city").find("option:selected").text()},
         success: function(data){
			alert(data);
		//	window.location.href="../default.htm";
         },
         error:function( data ){
			alert('询价失败');
         }
        });
		
		
		
			
				
		}
		
	}
	
	
	function check_mobile(mobile){
		var patrn = /(^0{0,1}1[3|4|5|6|7|8|9][0-9]{9}$)/; 
		if (!patrn.exec(mobile)){ 
			return false; 
		}
		 return true;
 	} 
</script>

